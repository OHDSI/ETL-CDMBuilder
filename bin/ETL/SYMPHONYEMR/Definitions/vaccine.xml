<QueryDefinition>
  <Query>
    SELECT
    patient_id
    , ecntr_id
    , ndc_cde
    , cvx_cde
    , vacin_nam
    , admrd_dte_tstmp
    from {sc}.vaccine
    JOIN {sc}._chunks ch ON ch.ChunkId = {0} AND patient_id = ch.PERSON_ID
    order by patient_id
  </Query>
  <DrugExposure>
    <DrugExposureDefinition>
      <PersonId>patient_id</PersonId>
      <StartDate>admrd_dte_tstmp</StartDate>
      <VisitOccurrenceId>ecntr_id</VisitOccurrenceId>
      <Concepts>
        <Concept>
          <ConceptIdMappers>
            <Mapper>
              <Lookup>Cvx</Lookup>
            </Mapper>
          </ConceptIdMappers>
          <Fields>
            <Field key="cvx_cde" defaultTypeId="10"/>
          </Fields>
        </Concept>
      </Concepts>
    </DrugExposureDefinition>
    <DrugExposureDefinition>
      <PersonId>patient_id</PersonId>
      <StartDate>admrd_dte_tstmp</StartDate>
      <VisitOccurrenceId>ecntr_id</VisitOccurrenceId>
      <Concepts>
        <Concept>
          <ConceptIdMappers>
            <Mapper>
              <Lookup>Ndc</Lookup>
            </Mapper>
          </ConceptIdMappers>
          <Fields>
            <Field key="ndc_cde" defaultTypeId="11"/>
          </Fields>
        </Concept>
      </Concepts>
    </DrugExposureDefinition>
    <DrugExposureDefinition>
      <PersonId>patient_id</PersonId>
      <StartDate>admrd_dte_tstmp</StartDate>
      <VisitOccurrenceId>ecntr_id</VisitOccurrenceId>
      <Concepts>
        <Concept>
          <ConceptIdMappers>
            <Mapper>
              <Lookup>Drug</Lookup>
            </Mapper>
          </ConceptIdMappers>
          <Fields>
            <Field key="vacin_nam" defaultTypeId="12"/>
          </Fields>
        </Concept>
      </Concepts>
    </DrugExposureDefinition>
  </DrugExposure>

</QueryDefinition>