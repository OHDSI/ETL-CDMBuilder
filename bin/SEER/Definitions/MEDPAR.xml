      <PaidTowardDeductible>PAID_TOWARD_DEDUCTIBLE</PaidTowardDeductible>
      <PaidByPayer>PAID_BY_PAYER</PaidByPayer>
      <PaidByCoordinationBenefits>PRIPYAMT</PaidByCoordinationBenefits>
