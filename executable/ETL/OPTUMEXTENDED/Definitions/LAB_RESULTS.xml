<QueryDefinition>
  <Query database="na">
    SELECT patid,
    loinc_cd,
    proc_cd,
    fst_dt,
    rslt_unit_nm unit_source_value,
    hi_nrml RANGE_HIGH,
    low_nrml RANGE_LOW,
    rslt_nbr VALUE_AS_NUMBER,
    rslt_txt VALUE_AS_STRING,
    pat_planid,
    labclmid as clmid
    FROM {sc}.LAB_RESULTS
    JOIN {sc}._chunks ch ON ch.ChunkId = {0} AND patid = ch.PERSON_ID
    order by PATID
  </Query>
  <Measurement>
    <MeasurementDefinition>
      <PersonId>patid</PersonId>
      <StartDate>fst_dt</StartDate>
      <ValueAsNumber>VALUE_AS_NUMBER</ValueAsNumber>
      <ValuesAsString>
        <string>VALUE_AS_STRING</string>
      </ValuesAsString>
      <RangeLow>RANGE_LOW</RangeLow>
      <RangeHigh>RANGE_HIGH</RangeHigh>
      <AdditionalFields>
        <string>pat_planid</string>
        <string>clmid</string>
      </AdditionalFields>
      <Concepts>
        <Concept>
          <ConceptIdMappers>
            <Mapper>
              <Lookup>Lab</Lookup>
            </Mapper>
          </ConceptIdMappers>
          <Fields>
            <Field key="loinc_cd" defaultTypeId="44818702"/>
          </Fields>
        </Concept>
        <Concept>
          <ConceptIdMappers>
            <Mapper>
              <Lookup>Lab_Units</Lookup>
            </Mapper>
          </ConceptIdMappers>
          <Fields>
            <Field key="unit_source_value" defaultTypeId="0" />
          </Fields>
        </Concept>
      </Concepts>
    </MeasurementDefinition>

    <MeasurementDefinition>
      <PersonId>patid</PersonId>
      <StartDate>fst_dt</StartDate>
      <ValueAsNumber>VALUE_AS_NUMBER</ValueAsNumber>
      <ValuesAsString>
        <string>VALUE_AS_STRING</string>
      </ValuesAsString>
      <RangeLow>RANGE_LOW</RangeLow>
      <RangeHigh>RANGE_HIGH</RangeHigh>
      <AdditionalFields>
        <string>pat_planid</string>
        <string>clmid</string>
      </AdditionalFields>
      <Concepts>
        <Concept>
          <ConceptIdMappers>
            <Mapper>
              <Lookup>Procedure</Lookup>
            </Mapper>
          </ConceptIdMappers>
          <Fields>
            <Field key="proc_cd" defaultTypeId="44818702"/>
          </Fields>
        </Concept>
        <Concept>
          <ConceptIdMappers>
            <Mapper>
              <Lookup>Procedure</Lookup>
            </Mapper>
          </ConceptIdMappers>
          <Fields>
            <Field key="unit_source_value" defaultTypeId="0" />
          </Fields>
        </Concept>
      </Concepts>
    </MeasurementDefinition>
  </Measurement>
</QueryDefinition>