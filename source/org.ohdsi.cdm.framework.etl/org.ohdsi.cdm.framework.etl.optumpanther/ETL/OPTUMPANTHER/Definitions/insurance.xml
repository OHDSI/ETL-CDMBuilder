<QueryDefinition>
  <Query>
    select
    ch.person_id as PersonId,
    i.ins_type,
    i.encid,
    i.insurance_date,
    i.insurance_time
    from {sc}.insurance i
    JOIN {sc}._chunks ch ON ch.ChunkId = {0} AND i.ptid = ch.PERSON_SOURCE_VALUE
    order by 1
  </Query>
  <Observation>
    <ObservationDefinition>
      <PersonId>PersonId</PersonId>
      <StartDate>insurance_date</StartDate>
      <Time>insurance_time</Time>
      <ValuesAsString>
        <string>ins_type</string>
      </ValuesAsString>
      <ValueSourceValue>ins_type</ValueSourceValue>
      <AdditionalFields>
        <string>encid</string>
      </AdditionalFields>
      <Concepts>
        <Concept>
          <Fields>
            <Field key="ins_type" defaultConceptId="0" defaultTypeId="45877222"/>
          </Fields>
        </Concept>
      </Concepts>
    </ObservationDefinition>
  </Observation>
</QueryDefinition>