<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0-windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="CdmFrameworkImport\Transformation\**" />
	  <EmbeddedResource Remove="CdmFrameworkImport\Transformation\**" />
	  <None Remove="CdmFrameworkImport\Transformation\**" />
	</ItemGroup>

	<ItemGroup>
	  <None Remove="CdmFrameworkImport\Queries\MSSQL\v5.4\CreateTables.sql" />
	  <None Remove="CdmFrameworkImport\Queries\MSSQL\v5.4\DropTables.sql" />
	  <None Remove="CdmFrameworkImport\Queries\MSSQL\v5.4\TruncateTables.sql" />
	  <None Remove="CdmFrameworkImport\Queries\MSSQL\v5.4\TruncateWithoutLookupTables.sql" />
	  <None Remove="CdmFrameworkImport\Queries\Postgre\v5.4\CreateTables.sql" />
	  <None Remove="CdmFrameworkImport\Queries\Postgre\v5.4\DropTables.sql" />
	  <None Remove="CdmFrameworkImport\Queries\Postgre\v5.4\TruncateTables.sql" />
	  <None Remove="CdmFrameworkImport\Queries\Postgre\v5.4\TruncateWithoutLookupTables.sql" />
	  <None Remove="CdmFrameworkImport\Queries\Transformation\JMDC\Annual_health_checkups.xml" />
	  <None Remove="CdmFrameworkImport\Queries\Transformation\JMDC\Enrollment.xml" />
	  <None Remove="CdmFrameworkImport\Queries\Transformation\OptumExtended\MEMBER_CONTINUOUS_ENROLLMENT.xml" />
	  <None Remove="CdmFrameworkImport\Queries\Transformation\OptumPanther\encounter.xml" />
	  <None Remove="CdmFrameworkImport\Queries\Transformation\Truven\ENROLLMENT_DETAIL_MDCD.xml" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\MSSQL\v5.4\CreateTables.sql">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\MSSQL\v5.4\DropTables.sql">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\MSSQL\v5.4\TruncateTables.sql">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\MSSQL\v5.4\TruncateWithoutLookupTables.sql">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Postgre\v5.4\CreateTables.sql">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Postgre\v5.4\DropTables.sql">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Postgre\v5.4\TruncateTables.sql">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Postgre\v5.4\TruncateWithoutLookupTables.sql">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Transformation\JMDC\Annual_health_checkups.xml">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Transformation\JMDC\Enrollment.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Transformation\OptumExtended\MEMBER_CONTINUOUS_ENROLLMENT.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Transformation\OptumPanther\encounter.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Queries\Transformation\Truven\ENROLLMENT_DETAIL_MDCD.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
	  <PackageReference Include="org.ohdsi.cdm.framework" Version="1.1.2" />
	  <PackageReference Include="org.ohdsi.cdm.framework.etl" Version="1.1.1" />
	  <PackageReference Include="Spectre.Console" Version="0.54.0" />
	</ItemGroup>

</Project>
