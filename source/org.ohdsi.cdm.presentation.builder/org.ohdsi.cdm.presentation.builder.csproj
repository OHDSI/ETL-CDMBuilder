<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0-windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="CdmFrameworkImport\DDL\MsSql\V5.4\CreateTables.sql" />
	  <None Remove="CdmFrameworkImport\DDL\MsSql\V5.4\TruncateTables.sql" />
	  <None Remove="CdmFrameworkImport\DDL\MySql\v5.4\CreateTables.sql" />
	  <None Remove="CdmFrameworkImport\DDL\MySql\v5.4\TruncateTables.sql" />
	  <None Remove="CdmFrameworkImport\DDL\Postgre\V5.4\CreateTables.sql" />
	  <None Remove="CdmFrameworkImport\DDL\Postgre\V5.4\TruncateTables.sql" />
	  <None Remove="CdmFrameworkImport\Transformation\Truven\FACILITY_HEADER.xml" />
	  <None Remove="CdmFrameworkImport\Transformation\Truven\INPATIENT_ADMISSIONS.xml" />
	  <None Remove="CdmFrameworkImport\Transformation\Truven\INPATIENT_SERVICES.xml" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="CdmFrameworkImport\DDL\MsSql\V5.4\CreateTables.sql">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\DDL\MsSql\V5.4\TruncateTables.sql">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\DDL\MySql\v5.4\CreateTables.sql">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\DDL\MySql\v5.4\TruncateTables.sql">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\DDL\Postgre\V5.4\CreateTables.sql">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\DDL\Postgre\V5.4\TruncateTables.sql">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Transformation\Truven\FACILITY_HEADER.xml">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Transformation\Truven\INPATIENT_ADMISSIONS.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="CdmFrameworkImport\Transformation\Truven\INPATIENT_SERVICES.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="org.ohdsi.cdm.framework" Version="1.1.1" />
	  <PackageReference Include="org.ohdsi.cdm.framework.etl" Version="1.1.0" />
	  <PackageReference Include="Spectre.Console" Version="0.54.0" />
	</ItemGroup>

</Project>
